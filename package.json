{
    "main": "server/dist/worker.js",
    "scripts": {
        "client": "webpack serve --config webpack.client.config.js",
        "server-webpack": "webpack -w --config webpack.server.config.js",
        "server-cloudworker": "nodemon --watch server/devdist/worker.js --exec cloudworker --debug --kv-file STORE=store.json server/devdist/worker.js",
        "server": "npm-run-all --parallel server-webpack server-cloudworker",
        "build-client": "rm -rf client/dist/* && webpack --mode production --config webpack.client.config.js",
        "build-server": "rm -rf server/dist/* && webpack --mode production --config webpack.server.config.js && cp server/dist/assets/* client/dist/assets/",
        "build": "npm-run-all build-client build-server",
        "td": "yarn build && wrangler publish --env test",
        "deploy": "yarn typecheck && yarn test && yarn build && wrangler publish --env live",
        "typecheck": "tsc -p server/tsconfig.json --noEmit && tsc -p client/tsconfig.json --noEmit",
        "test": "jest"
    },
    "dependencies": {
