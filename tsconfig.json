{
    "compilerOptions": {
        "strict": true,
        "noImplicitReturns": true,
        "allowJs": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "target": "es5",
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "dom",
            "es6",
            "esnext.asynciterable"
        ],
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ],
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "exclude": [
        "dist",
        "cfscripts",
        "server"
    ]
}