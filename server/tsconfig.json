{
    "compilerOptions": {
        "target": "esnext",
        "strict": true,
        "noImplicitReturns": true,
        "allowJs": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "esnext",
            "webworker"
        ],
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    }
}