{
    "main": "server/dist/worker.js",
    "scripts": {
        "client": "rm -rf client/devdist && parcel serve public/* -d client/devdist",
        "server-parcel": "parcel watch server/worker.tsx -d server/devdist",
        "server-cloudworker": "nodemon --watch server/devdist/worker.js --exec cloudworker --debug --kv-file STORE=store.json server/devdist/worker.js",
        "server": "npm-run-all --parallel server-parcel server-cloudworker",
        "build-client": "rm -rf client/dist/* && parcel build public/* -d client/dist",
        "build-server": "rm -rf server/dist/* && parcel build server/worker.tsx -d server/dist",
        "build": "npm-run-all --parallel build-client build-server",
        "td": "wrangler publish --env test",
        "deploy": "wrangler publish --env live",
        "typecheck": "cd server && tsc && cd ../client && tsc",
        "test": "jest"
    },
    "dependencies": {
        "@cloudflare/kv-asset-handler": "^0.0.7",
        "@cloudflare/workers-types": "^1.0.6",
        "@fortawesome/fontawesome-svg-core": "^1.2.26",
        "@fortawesome/free-solid-svg-icons": "^5.12.0",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "@sentry/browser": "5.15.4",
        "@stripe/stripe-js": "^1.5.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/classnames": "^2.2.10",
        "@types/cookie": "^0.3.3",
        "@types/html-entities": "^1.2.16",
        "@types/html-to-text": "^1.4.31",
        "@types/lodash": "^4.14.149",
        "@types/markdown-to-jsx": "^6.11.0",
        "@types/react": "^16.9.16",
        "@types/react-bootstrap": "^0.32.20",
        "@types/react-dom": "^16.9.4",
        "@types/react-router-dom": "^5.1.3",
        "@types/react-timeago": "^4.1.1",
        "@types/stripe-v3": "^3.1.11",
        "@types/url-join": "^4.0.0",
        "@types/uuid": "^3.4.6",
        "accessible-nprogress": "^2.0.1",
